msgstr "HTML ཡི་མིང་ཐོ"

msgid "Contextual filters"
msgstr "ནང་དོན་གོང་འོག་ཚག་རྒྱག་བྱེད"

msgid "contextual filters"
msgstr "ནང་དོན་གོང་འོག་ཚག་རྒྱག་བྱེད"

msgid "Contextual filter"
msgstr "ནང་དོན་གོང་འོག་ཚག་རྒྱག་བྱེད"

msgid "contextual filter"
msgstr "ནང་དོན་གོང་འོག་ཚག་རྒྱག་བྱེད"

msgid "filter criteria"
