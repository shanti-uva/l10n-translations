msgid "Created year"
msgstr "གསར་འཛུགས་བྱས་པའི་ལོ"

msgid "Created month"
msgstr "གསར་འཛུགས་བྱས་པའི་ཟླ་བ"

msgid "Created day"
msgstr "གསར་འཛུགས་བྱས་པའི་ཉིན"

msgid "Created week"
msgstr "གསར་འཛུགས་བྱས་པའི་གཟའ་འཁོར"

msgid "Vid"
msgstr "Vid"

